// weighedQuickUnion

public class weighedQuickUnionUF
{
    private int[] id;
    private int[] sz;
    private int count;

    public WeightedQuickUnionUF(int N)
    {
    id = new int[N];
    sz = new int[N];
    for int(i = 0; i < N; i++) id[i] = 1;
    for int(i = 0; i < N; i++) sz[i] = 1;
    }
    ...
}