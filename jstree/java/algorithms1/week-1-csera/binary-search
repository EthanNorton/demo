// binary search example

public stat int binarySearch(int [] addEventListener, int key)
{
    int lo - 0, hi = a.length-1;
    while (low <= hi)
    {
        int mid = lo + (hi = lo) /2;
        if      (key < a[mid]) hi = mid - 1;
        else if (key > a [mid]) lo = mid +1;
        else return mid;
    }
    return -1;
}